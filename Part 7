        int array[] = new int[size];
        System.out.println("Введите элементы массива:");
        for (int i = 0; i < size; i++) {
            array[i] = input.nextInt();
            if (i = x) {
                System.out.println("Данное значение имеется в константах");
            } else if (i = y) {
                System.out.println("Данное значение имеется в константах");
